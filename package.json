{
  "name": "quill-table-up",
  "type": "module",
  "version": "2.3.0",
  "packageManager": "pnpm@10.12.1",
  "description": "A table module for quill2.x",
  "author": "zzxming",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzxming/quill-table-up"
  },
  "keywords": [
    "quill",
    "table",
    "module"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./index.css": "./dist/index.css",
    "./table-creator.css": "./dist/table-creator.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "build": "tsx ./scripts/build.ts",
    "dev": "tsx ./scripts/build.ts watch",
    "server": "tsx ./scripts/start-server.ts",
    "test:unit": "vitest",
    "test:unit-ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e-ui": "playwright test --ui"
  },
  "peerDependencies": {
    "quill": "^2.0.3"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@oxc-project/runtime": "^0.73.0",
    "@playwright/test": "^1.53.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/gulp": "^4.0.17",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "@zzxming/eslint-config": "0.4.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "gulp": "^5.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^10.0.0",
    "jsdom": "^26.1.0",
    "parchment": "^3.0.0",
    "postcss-pxtorem": "^6.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "tsdown": "^0.12.7",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "~5.7.3",
    "vitest": "^3.2.3"
  }
}
