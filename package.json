{
  "name": "quill-table-up",
  "type": "module",
  "version": "3.0.1",
  "packageManager": "pnpm@10.15.0",
  "description": "A table module for quill2.x",
  "author": "zzxming",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzxming/quill-table-up"
  },
  "keywords": [
    "quill",
    "table",
    "module"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./index.css": "./dist/index.css",
    "./table-creator.css": "./dist/table-creator.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "build": "tsx ./scripts/build.ts",
    "dev": "tsx ./scripts/build.ts watch",
    "server": "tsx ./scripts/start-server.ts",
    "test:unit": "vitest",
    "test:unit-ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e-ui": "playwright test --ui"
  },
  "peerDependencies": {
    "quill": "^2.0.3"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@oxc-project/runtime": "^0.84.0",
    "@playwright/test": "^1.55.0",
    "@prettier/plugin-xml": "^3.4.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/gulp": "^4.0.17",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@zzxming/eslint-config": "0.5.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "gulp": "^5.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^10.0.0",
    "jsdom": "^26.1.0",
    "parchment": "^3.0.0",
    "postcss-pxtorem": "^6.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "tsdown": "^0.14.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "ws": "^8.18.3"
  }
}
